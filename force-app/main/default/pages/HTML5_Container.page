<apex:page docType="html-5.0" applyHtmlTag="false" applyBodyTag="false"
           showHeader="false" sidebar="false" standardStylesheets="false" standardController="Account">
<html>
    <head>
        <title>HTML5 Container Page</title>
    </head>
    <body>
        <h1>An Almost Empty Page</h1>
        <p>This is a very simple page.</p>
        
        <apex:outputField value="{!account.RecordTypeId}"/>
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script>
            // Shorthand for $( document ).ready()
            $(function() {
                console.log( "document loaded" );
            });
         
            $( window ).on( "load", function() {
                console.log( "window loaded" );
            });
        </script>
    </body>
</html>
</apex:page>